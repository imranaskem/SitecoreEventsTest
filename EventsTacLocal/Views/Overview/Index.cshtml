@model EventsTacLocal.Models.OverviewList

<!-- [Category List] -->
<ul class="block-grid-md-4 block-grid-sm-3 block-grid-xs-1">
    @foreach (var overview in Model)
    {
        <li>
            <div class="thumbnail">
                <a href="@overview.Url">
                    @overview.Image
                </a>
                <div class="caption">
                    <h3 class="teaser-heading">@overview.Title</h3>
                    <a href="@overview.Url" class="btn btn-default">@Model.ReadMore</a>
                </div>
            </div>  
        </li>
    }
</ul>
<!-- [Category List] -->